                <Image Source="{StaticResource DeleteFileDrawingImage}" />
            </MenuItem.Icon>
        </MenuItem>
    </ContextMenu>

    <ContextMenu x:Key="StratisNetworkContextMenu" Style="{DynamicResource ContextMenuStyle}">
        <MenuItem Header="Statistics" Command="{x:Static stratisui:BlockchainExplorerTree.NewEndpointCmd}">
            <MenuItem.Icon>
                <Image Source="{StaticResource GlobeDrawingImage}" />
            </MenuItem.Icon>
        </MenuItem>
    </ContextMenu>

    <ContextMenu x:Key="RootContextMenu" Style="{DynamicResource ContextMenuStyle}">
        <MenuItem Header="Add Network..." Command="{x:Static stratisui:BlockchainExplorerTree.NewNetworkCmd}">
            <MenuItem.Icon>
                <Image Source="{DynamicResource AddNetworkDrawingImage}" />
            </MenuItem.Icon>
        </MenuItem>
        <MenuItem Header="Add Folder..." Command="{x:Static stratisui:BlockchainExplorerTree.NewFolderCmd}">
            <MenuItem.Icon>
                <Image Source="{DynamicResource AddFolderDrawingImage}" />
            </MenuItem.Icon>
        </MenuItem>
    </ContextMenu>

    <ContextMenu x:Key="UserFolderContextMenu" Style="{DynamicResource ContextMenuStyle}">
        <MenuItem Header="Add Network..." Command="{x:Static stratisui:BlockchainExplorerTree.NewNetworkCmd}">
            <MenuItem.Icon>
                <Image Source="{DynamicResource AddNetworkDrawingImage}" />
            </MenuItem.Icon>
        </MenuItem>
        <MenuItem Header="Add Folder..." Command="{x:Static stratisui:BlockchainExplorerTree.NewFolderCmd}">
            <MenuItem.Icon>
                <Image Source="{DynamicResource AddFolderDrawingImage}" />
            </MenuItem.Icon>
        </MenuItem>
        <MenuItem Header="Delete" Command="{x:Static stratisui:BlockchainExplorerTree.DeleteFolderCmd}">
            <MenuItem.Icon>
                <Image Source="{DynamicResource DeleteFolderDrawingImage}" />
            </MenuItem.Icon>
        </MenuItem>
    </ContextMenu>


    <!-- sorts categories by names in ascending order -->
    <cm:SortDescriptionCollection x:Key="AscendingNames">
        <cm:SortDescription PropertyName="Header.CategoryName" Direction="Ascending" />
    </cm:SortDescriptionCollection>

    <!-- sorts categories by names in descending order -->
    <cm:SortDescriptionCollection x:Key="DescendingNames">
        <cm:SortDescription PropertyName="Header.CategoryName" Direction="Descending" />
    </cm:SortDescriptionCollection>

    <!-- Dialog style -->
    <Style x:Key="DialogStyle" TargetType="{x:Type StackPanel}">
        <Style.Resources>
            <Style TargetType="{x:Type ui:TextBox}">
                <Setter Property="FontSize" Value="12" />
            </Style>
            <Style TargetType="{x:Type ui:NumberBox}">
                <Setter Property="FontSize" Value="12" />
                <Setter Property="HorizontalContentAlignment" Value="Right" />
            </Style>
            <Style TargetType="{x:Type ui:TextBlock}">
                <Setter Property="FontSize" Value="12" />
            </Style>
        </Style.Resources>
    </Style>

    <!-- Dialogs -->
    <StackPanel x:Key="AddNetworkDialog" Orientation="Vertical" VerticalAlignment="Top" Style="{DynamicResource DialogStyle}">
        <StackPanel Orientation="Vertical">
            <ui:TextBlock Text="Enter the network name:" VerticalAlignment="Center"  Margin="1,0,0,2" />
            <ui:TextBox x:Name="NetworkNameTextBox" Width="150" Margin="0,2,10,4" VerticalAlignment="Center"/>
        </StackPanel>
        <StackPanel Orientation="Vertical" >
            <ui:TextBlock Text="JSON-RPC Url:" VerticalAlignment="Center" Margin="1,0,0,2" />
            <ui:TextBox x:Name="NetworkRpcUrlTextBox"  Width="150" Margin="0,2,10,4" VerticalAlignment="Center" />
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <ui:TextBlock Text="Network chain id (optional):" VerticalAlignment="Center" Margin="1,0,0,2" />
            <ui:NumberBox SpinButtonPlacementMode="Hidden" Icon="{ui:SymbolIcon NumberSymbolSquare24}" Name="NetworkChainIdNumberBox" Width="150" Margin="0,2,10,4" VerticalAlignment="Center"/>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Grid>
                <ui:TextBlock Name = "AddNetworkDialogValidationErrors" Grid.Row="0" Foreground="Red" Visibility="Hidden" VerticalAlignment="Center" TextWrapping="WrapWithOverflow" 
                            ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True"/>
                <ui:ProgressRing Name="AddNetworkDialogProgressRing" IsIndeterminate="True" IsEnabled="True" Width="20" Height="20" Visibility="Hidden" />
            </Grid>
        </StackPanel>
    </StackPanel>

    <StackPanel x:Key="AddEndpointDialog" Orientation="Vertical" VerticalAlignment="Top" Style="{DynamicResource DialogStyle}">
        <StackPanel Orientation="Vertical" >
            <ui:TextBlock Text="JSON-RPC Url:" VerticalAlignment="Center" Margin="1,0,0,2" />
            <ui:TextBox x:Name="EndpointRpcUrlTextBox"  Width="150" Margin="0,2,10,4" VerticalAlignment="Center" />
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <ui:TextBlock Name = "AddEndpointDialogValidationErrors" Foreground="Red" Visibility="Hidden" VerticalAlignment="Center" TextWrapping="WrapWithOverflow" />
        </StackPanel>
    </StackPanel>

    <StackPanel x:Key="AddFolderDialog" Orientation="Vertical" VerticalAlignment="Top" Style="{DynamicResource DialogStyle}">
        <StackPanel Orientation="Vertical" >
            <ui:TextBlock Text="Folder name:" VerticalAlignment="Center" Margin="1,0,0,2" />
            <ui:TextBox x:Name="FolderNameTextBox"  Width="150" Margin="0,2,10,4" VerticalAlignment="Center" />
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <ui:TextBlock Name = "AddFolderDialogValidationErrors" Foreground="Red" Visibility="Hidden" VerticalAlignment="Center" TextWrapping="WrapWithOverflow" />
        </StackPanel>
    </StackPanel>

    <StackPanel x:Key="EditAccountDialog" Orientation="Vertical" VerticalAlignment="Top" Style="{DynamicResource DialogStyle}">
        <StackPanel Orientation="Vertical" >
            <ui:TextBlock Text="Account Public Key:" VerticalAlignment="Center" Margin="1,0,0,2" />
            <ui:TextBlock x:Name="AccountPublicKeyTextBox" TextWrapping="Wrap" ScrollViewer.HorizontalScrollBarVisibility="Visible" ScrollViewer.VerticalScrollBarVisibility="Visible" ScrollViewer.CanContentScroll="True"  Width="160" Height="75" VerticalAlignment="Center" Margin="1,0,0,2"   />
            <ui:TextBlock Text="Account Label:" VerticalAlignment="Center" Margin="1,0,0,2" />
            <ui:TextBox x:Name="AccountLabelTextBox"  Width="150" Margin="0,2,10,4" VerticalAlignment="Center" />
        </StackPanel>
    </StackPanel>

    <StackPanel x:Key="AddDeployProfileDialog" Orientation="Vertical" VerticalAlignment="Top" Style="{DynamicResource DialogStyle}">
        <StackPanel Orientation="Vertical" >
            <ui:TextBlock Text="Deploy Profile Name:" VerticalAlignment="Center" Margin="1,0,0,2" />
            <ui:TextBox x:Name="DeployProfileNameTextBox"  Width="150" Margin="0,2,10,4" VerticalAlignment="Center" />
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <ui:TextBlock Text="Select the deployment endpoint:" VerticalAlignment="Center"  Margin="1,0,0,2" />
            <ComboBox x:Name="DeployProfileEndpointListView" Width="150" Margin="0,2,10,4" VerticalAlignment="Center"/>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <ui:TextBlock Text="Select the account to use:" VerticalAlignment="Center"  Margin="1,0,0,2" />
            <ComboBox x:Name="DeployProfileAccountListView" Width="150" Margin="0,2,10,4" VerticalAlignment="Center"/>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <ui:TextBlock Text="Account private key (optional):" VerticalAlignment="Center" Margin="1,0,0,2" />
            <ui:PasswordBox Name="DeployProfileAccountPrivateKeyPasswordBox" Width="150" Margin="0,2,10,4" VerticalAlignment="Center"/>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Grid>
                <ui:TextBlock Name = "AddDeployProfileDialogValidationErrors" Grid.Row="0" Foreground="Red" Visibility="Hidden" VerticalAlignment="Center" TextWrapping="WrapWithOverflow" 
                        ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True"/>
            </Grid>
        </StackPanel>
    </StackPanel>
</ResourceDictionary>