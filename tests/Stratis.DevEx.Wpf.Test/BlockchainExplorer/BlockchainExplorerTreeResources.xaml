    <!-- TreeView items data template-->
    <DataTemplate x:Key="BlockchainInfoTemplate" DataType="{x:Type vm:BlockchainInfo}">
        <StackPanel x:Name="GroupPanel" Orientation="Horizontal" Margin="0,2,0,2">
            <ui:Image x:Name="NodeImage" Width="16" Height="16" Source="{StaticResource StratisIconDrawingImage}"/>
            <ui:TextBlock x:Name="NodeText" Text="{Binding Path=DisplayName}" Margin="8,0,0,0" TextWrapping="Wrap" />
        </StackPanel>
        <DataTemplate.Triggers>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=Kind}" Value="Folder" />
                    <Condition Binding="{Binding Path=Name}" Value="EVM Networks" />
                </MultiDataTrigger.Conditions>
                <Setter TargetName="NodeImage" Property="Source" Value="{StaticResource BlockchainNetworkDrawingImage}" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=Kind}" Value="Folder" />
                    <Condition Binding="{Binding Path=Name}" Value="Endpoints" />
                </MultiDataTrigger.Conditions>
                <Setter TargetName="NodeImage" Property="Source" Value="{StaticResource GlobeDrawingImage}" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=Kind}" Value="Folder" />
                    <Condition Binding="{Binding Path=Name}" Value="Accounts" />
                </MultiDataTrigger.Conditions>
                <Setter TargetName="NodeImage" Property="Source" Value="{StaticResource AccountsDrawingImage}" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=Kind}" Value="Folder" />
                    <Condition Binding="{Binding Path=Name}" Value="Deploy Profiles" />
                </MultiDataTrigger.Conditions>
                <Setter TargetName="NodeImage" Property="Source" Value="{StaticResource DeployProfileDrawingImage}" />
            </MultiDataTrigger>
            <DataTrigger Binding="{Binding Path=Kind}" Value="Network">
                <Setter TargetName="NodeText" Property="ToolTipService.InitialShowDelay" Value="100" />
                <Setter TargetName="NodeText" Property="ToolTipService.Placement" Value="MousePoint" />
                <Setter TargetName="NodeText" Property="ToolTipService.ToolTip" Value="{Binding Path=ToolTip}" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=Kind}" Value="Account">
                <Setter TargetName="NodeText" Property="ToolTipService.InitialShowDelay" Value="100" />
                <Setter TargetName="NodeText" Property="ToolTipService.Placement" Value="MousePoint" />
                <Setter TargetName="NodeText" Property="ToolTipService.ToolTip" Value="{Binding Path=ToolTip}" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=Kind}" Value="Endpoint">
                <Setter TargetName="NodeImage" Property="Source" Value="{StaticResource UrlDrawingImage}" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=Kind}" Value="Account">
                <Setter TargetName="NodeImage" Property="Source" Value="{StaticResource AccountDrawingImage}" />
            </DataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=Kind}" Value="Network" />
                    <Condition Binding="{Binding Path=Data[ChainId]}" Value="105105" />
                </MultiDataTrigger.Conditions>
                <Setter TargetName="NodeImage" Property="Source" Value="{StaticResource StratisIconDrawingImage}" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=Kind}" Value="Network" />
                    <Condition Binding="{Binding Path=Data[ChainId]}" Value="1337" />
                </MultiDataTrigger.Conditions>
                <Setter TargetName="NodeImage" Property="Source" Value="{StaticResource GanacheDrawingImage}" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=Kind}" Value="UserFolder" />
                    <Condition Binding="{Binding Path=IsExpanded, RelativeSource={RelativeSource TemplatedParent}}" Value="True" />
                </MultiDataTrigger.Conditions>
                <Setter TargetName="NodeImage" Property="Source" Value="{StaticResource FolderOpenDrawingImage}" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=Kind}" Value="UserFolder" />
                    <Condition Binding="{Binding Path=IsExpanded, RelativeSource={RelativeSource TemplatedParent}}" Value="False" />
                </MultiDataTrigger.Conditions>
                <Setter TargetName="NodeImage" Property="Source" Value="{StaticResource FolderDrawingImage}" />
            </MultiDataTrigger>
        </DataTemplate.Triggers>
    </DataTemplate>

    <!-- Tree items style -->
    <Style x:Key="TreeViewItemStyle" TargetType="{x:Type TreeViewItem}">
        <Setter Property="HeaderTemplate" Value="{StaticResource BlockchainInfoTemplate}" />
        <Setter Property="Foreground" Value="{DynamicResource {x:Static stratisui:VSTheme.ToolWindowTextKey}}"/>
        <Setter Property="Background" Value="{DynamicResource {x:Static stratisui:VSTheme.ToolWindowBackgroundKey}}"/>
    </Style>

    <Style x:Key="TreeViewStyle" TargetType="{x:Type TreeView}">
        <Setter Property="Margin" Value="0,20,0,0" />
    </Style>

    <!-- Context menu style -->
    <Style x:Key="ContextMenuStyle" TargetType="{x:Type ContextMenu}">
        <Setter Property="Foreground" Value="{DynamicResource {x:Static stratisui:VSTheme.ToolWindowTextKey}}"/>
        <Setter Property="Background" Value="{DynamicResource {x:Static stratisui:VSTheme.ToolWindowBackgroundKey}}"/>
        <Setter Property="Panel.Background" Value="{DynamicResource {x:Static stratisui:VSTheme.ToolWindowBackgroundKey}}"/>
        <Style.Resources>
            <Style TargetType="{x:Type Image}">
                <Setter Property="Height" Value="16" />
                <Setter Property="Width" Value="16" />
                <Setter Property="Margin" Value="-8,2,8,0" />
            </Style>

            <Style TargetType="{x:Type MenuItem}">
                <Setter Property="Foreground" Value="{DynamicResource {x:Static stratisui:VSTheme.ToolWindowTextKey}}"/>
                <Setter Property="Background" Value="{DynamicResource {x:Static stratisui:VSTheme.ToolWindowBackgroundKey}}"/>
                <Setter Property="TextElement.Background" Value="{DynamicResource {x:Static stratisui:VSTheme.ToolWindowBackgroundKey}}"/>
            </Style>
        </Style.Resources>
    </Style>

    <!-- Context Menus -->
    <ContextMenu x:Key="NetworkContextMenu" Style="{DynamicResource ContextMenuStyle}">
        <MenuItem Header="Add Endpoint..." Command="{x:Static stratisui:BlockchainExplorerTree.NewEndpointCmd}">
            <MenuItem.Icon>
                <Image Source="{StaticResource GlobeDrawingImage}" />
            </MenuItem.Icon>
        </MenuItem>
        <MenuItem Header="Add Deploy Profile..." Command="{x:Static stratisui:BlockchainExplorerTree.NewDeployProfileCmd}">
            <MenuItem.Icon>
                <Image Source="{StaticResource AddDeployProfileDrawingImage}" />
            </MenuItem.Icon>
        </MenuItem>
        <MenuItem Header="Delete" Command="{x:Static stratisui:BlockchainExplorerTree.DeleteNetworkCmd}">
            <MenuItem.Icon>
                <Image Source="{StaticResource DeleteNetworkDrawingImage}" />
            </MenuItem.Icon>
        </MenuItem>
    </ContextMenu>

    <ContextMenu x:Key="EndpointsFolderContextMenu" Style="{DynamicResource ContextMenuStyle}">
        <MenuItem Header="Add Endpoint..." Command="{x:Static stratisui:BlockchainExplorerTree.NewEndpointCmd}">
            <MenuItem.Icon>
                <Image Source="{StaticResource GlobeDrawingImage}" />
            </MenuItem.Icon>
        </MenuItem>
    </ContextMenu>

    <ContextMenu x:Key="EndpointContextMenu" Style="{DynamicResource ContextMenuStyle}">
        <MenuItem Header="Delete" Command="{x:Static stratisui:BlockchainExplorerTree.DeleteEndpointCmd}">
